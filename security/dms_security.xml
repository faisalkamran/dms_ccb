	<!-- ****** -->

	<!-- Category definition for File object -->
	<record id="dms_ccb_file_obj_category" model="ir.module.category">
		<field name="name">CCB File Obj</field>
	</record>

	<!-- DMS CCB User Group definition here for File object -->
	<record id="dms_ccb_file_obj_user_group" model="res.groups">
		<field name="name">User</field>
		<field name="category_id" ref="dms_ccb_file_obj_category"></field>
		<field name="implied_ids"
				eval="[(4, ref('base.group_user'))]"></field>
